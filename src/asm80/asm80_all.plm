		if not b6B35 and op > 3 then
			call syntaxError;

		if op = 0 then
			op = opType;
		else
			opSP = opSP - 1;
		

		if (b6B28 := b4181(op)) then
		do;
			accum2 = sub$43DD;
			b6856 = b6855;
			w685C = w685A;
			b6859 = valType;
		end;

