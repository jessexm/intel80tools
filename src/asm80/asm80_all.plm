	add	l
	mov	l, a
	mvi	a, 0
	adc	h
	mov	h, a
	dcr	b	; get the 3 chars
	jnz	L6291
	ret

pack1:	ldax	d		; pick up next character
	inx	d		; advance for next time
	dcr	c		; check it is valid
	jm	L62B7
	sui	2Fh		; /012345678  (0-9)
				; 9?@ABCDEFG  (10-19)
				; HIJKLMNOPQ  (20-29)
				; RSTUVWXYZ[  (30-39)
	cpi	10h		; digit so return
	rc
	sui	5		; exclude : ; < = >
	ret

L62B7:
	sub	a		; 0 no char
	ret

	end
