	ii = 0;

	do while not isRParen and ii < 4;
		asmax$ref(ii) = curChar;
		ii = ii + 1;
		curChar = getCh;
	end;

	if isRParen or isWhite then
		if sub$6F07(.asmax$ref) then
			return;
	curFileName$p = .asmax$ref;
