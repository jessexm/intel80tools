PLM: DO;
$include(main.ipx)
DECLARE
	copyRight(*) BYTE DATA('(C) 1976, 1977, 1982 INTEL CORP');

DECLARE state$main BYTE;

	fatalError: PROCEDURE(code) PUBLIC;
		DECLARE code BYTE;
		if code = ERR83 then
			call fatal(.('DYNAMIC STORAGE OVERFLOW'), 24);
		call fatal(.('UNKNOWN FATAL ERROR'), 19);
		call Exit;
	end fatalError;

	state$main = 10;
	call sub$40AC;
	state$main = 15;
	call sub$4767;
	call chain(.invokeName);
end;

