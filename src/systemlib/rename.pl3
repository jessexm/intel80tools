RENAME: DO;
isis: procedure (type, parameter$ptr) external;
    declare type byte,
            parameter$ptr address;
end isis;

declare I$RENAME literally '7';

rename: procedure (old$p, new$p, status$p) public;
    declare (old$p, new$p, status$p) address;

    call isis(I$RENAME, .old$p);

end rename;

end;
