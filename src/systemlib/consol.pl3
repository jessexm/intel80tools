CONSOL: DO;
isis: procedure (type, parameter$ptr) external;
    declare type byte,
            parameter$ptr address;
end isis;

declare I$CONSOL literally '8';

consol: procedure (ci$p, co$p, status$p) public;
    declare (ci$p, co$p, status$p) address;

    call isis(I$CONSOL, .ci$p);
end consol;

end;

