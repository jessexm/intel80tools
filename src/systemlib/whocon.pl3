WHOCON: DO;
isis: procedure (type, parameter$ptr) external;
    declare type byte,
            parameter$ptr address;
end isis;

declare I$WHOCON literally '13';

whocon: procedure (conn, buff$p) public;
    declare (conn, buff$p) address;
    declare status address;

    status = .status;
    call isis(I$WHOCON, .conn);

end whocon;

end;

