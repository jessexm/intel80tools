strtst:
DO;
strequ: PROCEDURE(str1p, str2p, cnt) BYTE PUBLIC;
	DECLARE (str1p, str2p) ADDRESS;
	DECLARE cnt BYTE;
	DECLARE i BYTE;
	DECLARE str1 BASED str1p (1) BYTE;
	DECLARE str2 BASED str2p (1) BYTE;


	cnt = cnt - 1;
	DO i = 0 TO cnt;
		IF str1(i) <> str2(i) THEN
			RETURN 0;
	END;
	RETURN 0FFH;
END strequ;
END;
