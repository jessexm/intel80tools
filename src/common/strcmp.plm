strcmp: DO;

strncmp: PROCEDURE(s1$p, s2$p, cnt) BYTE PUBLIC;
	DECLARE s1$p ADDRESS, s2$p ADDRESS, cnt BYTE;
	DECLARE s1 BASED s1$p BYTE, s2 BASED s2$p BYTE;
	do while cnt <> 0;
		if s1 < s2 then
			return 1;
		if s1 > s2 then
			return 2;
		cnt = cnt - 1;
		s1$p = s1$p + 1;
		s2$p = s2$p + 1;
	end;
	return 0;
end strncmp;


END;
