cpyTill: DO;

cpyTill: PROCEDURE(src$p, dst$p, cnt, endch) PUBLIC;
	DECLARE src$p ADDRESS, dst$p ADDRESS, cnt ADDRESS, endch BYTE;
	DECLARE src BASED src$p BYTE, dst BASED dst$p BYTE;

	do while cnt <> 0;
		if src = endch then
			return;
		dst = src;
		src$p = src$p + 1;
		dst$p = dst$p + 1;
		cnt = cnt - 1;
	end;
end cpyTill;
END;
