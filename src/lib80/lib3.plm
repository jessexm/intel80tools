lib3:
DO;

skipSpc: PROCEDURE(ch$p) ADDRESS PUBLIC;
	DECLARE ch$p ADDRESS;
	DECLARE ch BASED ch$p BYTE;

	DO WHILE ch = ' ';
		ch$p = ch$p + 1;
	END;
	RETURN ch$p;
END skipSpc;

pastFileName: PROCEDURE(ch$p) ADDRESS PUBLIC;
	DECLARE ch$p ADDRESS;
	DECLARE ch BASED ch$p BYTE;

	DO WHILE ch = ':' OR ch = '.' OR (ch >= '0' AND ch <= '9') OR (ch >= 'A' AND ch <= 'Z');
		ch$p = ch$p + 1;
	END;
	RETURN ch$p;
END pastFileName;
end;

